{
  "manifest_version": 3,
  "name": "Open Kagi Translate",
  "version": "1.0",
  "description": "Opens the Kagi Translate page in a new tab",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Kagi Translate",
    "default_icon": {
      "16": "icon.svg",
      "48": "icon.svg"
    }
  },
  "icons": {
    "16": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  }
}